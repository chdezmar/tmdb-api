<div class="col-md-4"></div>
<div class="col-md-4">
  <h1>Log In</h1>
    <div class="space"></div>
    <%= form_with url: sessions_path do %>
        <div class="field">
          <%= label_tag :email %><br />
          <%= text_field_tag :email, params[:email] %>
        </div>
        <div class="field">
          <%= label_tag :password %><br />
          <%= password_field_tag :password%>
        </div>
        <br>
        <div class="actions"><%= submit_tag "Log In" %></div>
    <% end %>
</div>
<div class="col-md-4"></div>